@model IEnumerable<BO.Student>

@{
    ViewBag.Title = "DisplayStudent";
}

<h1>Display Student</h1>
<table class=" table">
    <tr>
        <th>
            @Html.DisplayName("Name")
        </th>
        <th>
            @Html.DisplayNameFor(ln => ln.LName)
        </th>
        <th>
            @Html.DisplayNameFor(dep=>dep.DepName)
       </th>
        <th>
            @Html.DisplayNameFor(dob =>dob.DOB)

        </th>
        <th>
            @Html.DisplayNameFor(ph => ph.Phone)
      </th>
        <th>
            @Html.DisplayNameFor(email=>email.Email)
        </th>
        <th>
            @Html.DisplayNameFor(gender=>gender.Gender)
        </th>
        <th>
            @Html.DisplayNameFor(address=>address.Address)
        </th>
        <th>
            @Html.DisplayNameFor(city=>city.CityName),@Html.DisplayNameFor(state=>state.StateName)
        </th>
        <th>
            @Html.DisplayNameFor(uni=>uni.UniversityName)
        </th>
        <th>
            @Html.DisplayNameFor(hobbies=>hobbies.Hobbies)
        </th>
        <th>
            @Html.DisplayNameFor(pic=>pic.ProfilePicName)
        </th>
    </tr>
    @foreach(BO.Student obj in Model)
            {
<tr>
    <td>
        @Html.DisplayFor(fn => obj.FName)&nbsp;@Html.DisplayFor(ln => obj.LName)
   </td>
    <td>
        @Html.DisplayFor(dep => obj.DepName)
    </td>
    <td>
        @Html.DisplayFor(dob => obj.DOB)

    </td>
    <td>
        @Html.DisplayFor(ph => obj.Phone)
    </td>
    <td>
     @Html.DisplayFor(email=>obj.Email)
    </td>
    <td>
        @Html.DisplayFor(gender=>obj.Gender)
    </td>
    <td>
        @Html.DisplayFor(address=>obj.Address)
    </td>
    <td>
        @Html.DisplayFor(city=>obj.CityName),@Html.DisplayFor(state=>obj.StateName)
    </td>
    <td>
        @Html.DisplayFor(uni=>obj.UniversityName)
    </td>
    <td>
        @Html.DisplayFor(hobbies=>obj.Hobbies)
    </td>
    <td>
        <img src="https://bsite.net/mahiansari/ProfilePics/@obj.ProfilePicName " width="50" height="50" />
    </td>
</tr>
  }
</table>

